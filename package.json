{
  "name": "electron-vue-vite",
  "version": "1.0.0",
  "main": "dist/main/index.cjs",
  "license": "MIT",
  "lint-staged": {
    "**.{js,tsx?,vue,css,scss,html}": [
      "prettier --write "
    ]
  },
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "prebuild": "vue-tsc --project packages/renderer/tsconfig.json --noEmit && node scripts/build.mjs",
    "build": "electron-builder",
    "debug": "npm run prebuild && vite ./packages/renderer",
    "postinstall": "husky install",
    "clean-pycache": "rimraf ./**/__pycache__"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.181",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "electron": "16.0.8",
    "electron-builder": "^22.14.5",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "typescript": "^4.5.5",
    "unocss": "^0.27.1",
    "unplugin-vue-components": "^0.18.0",
    "vite": "^2.8.6",
    "vite-plugin-resolve": "^1.8.0",
    "vue-tsc": "^0.31.1"
  },
  "env": {
    "//": "Used in build scripts",
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 25252
  },
  "keywords": [
    "vite",
    "electron",
    "vue3",
    "rollup"
  ],
  "dependencies": {
    "@vueuse/core": "^8.2.6",
    "axios": "^0.26.0",
    "pinia": "^2.0.11",
    "vue": "^3.2.29",
    "vue-router": "^4.0.13",
    "vuedraggable": "^4.1.0"
  }
}
