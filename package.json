{
  "name": "dcalc-electron",
  "version": "0.0.1",
  "main": "dist/main/index.cjs",
  "license": "MIT",
  "lint-staged": {
    "**.{js,tsx?,vue,css,scss,html}": [
      "prettier --write "
    ]
  },
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "web": "vite ./packages/renderer",
    "prebuild": "pnpm -r build",
    "build": "electron-builder",
    "debug": "npm run prebuild && vite ./packages/renderer",
    "postinstall": "husky install",
    "clean-pycache": "rimraf ./**/__pycache__",
    "clean-deps": "rimraf ./**/node_modules",
    "release": "bumpp"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "devDependencies": {
    "bumpp": "^7.1.1",
    "electron": "16.0.8",
    "electron-builder": "^22.14.5",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5",
    "vite": "^2.9.9"
  },
  "env": {
    "//": "Used in build scripts",
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 25252
  },
  "keywords": [
    "vite",
    "electron",
    "vue3",
    "rollup"
  ]
}
